#!/bin/env node
// New Relic RPM
if(process.env.NEW_RELIC_LICENSE_KEY) {
  require('newrelic');
}

// Load configuration
var config = require("../config/configuration.js");
var server = require('../app.js');

// Start the server
server.listen(config.port, function() {
  console.log("Server listening on " + server.url);
});
